<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="320.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.indexer.controllers.filesearch.FileSizeFilterController">
   <children>
      <VBox alignment="CENTER" prefHeight="320.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXComboBox fx:id="fileSizeComboBox" prefWidth="100.0">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="less" />
                           <String fx:value="less or equal" />
                           <String fx:value="equal" />
                           <String fx:value="greater" />
                           <String fx:value="greater or equal" />
                        </FXCollections>
                     </items>
                  </JFXComboBox>
                  <JFXTextField fx:id="fileSizeTextField">
                     <HBox.margin>
                        <Insets left="10.0" right="2.0" />
                     </HBox.margin>
                  </JFXTextField>
                  <Label text="kB" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
